{
    "manifest_version": 2,
    "name": "Coroutine's Breakout",
    "description": "Works on any extension page that loads a script, so any mv2 background page or any mv3 extension page that has <script src='blah'></script>",
    "version": "0.0.1",
    "key": "",
    "permissions": [
        "debugger"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    }
}
